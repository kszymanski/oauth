<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://oauthapi.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>oauthapi.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>OauthApi</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$OauthApi</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="AuthContext" Order="1" Enabled="True">
          <Destination Path="Data Source=oauth.database.windows.net;Initial Catalog=oauthdb;Persist Security Info=True;User ID=api;Password=Antymateria1410" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="OAuthExample.Api.Entity.AuthContext, OAuthExample.Api" MigrationConfiguration="OAuthExample.Api.Migrations.Configuration, OAuthExample.Api" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)AuthContext-Web.config Connection String">
      <ParameterValue>Data Source=oauth.database.windows.net;Initial Catalog=oauthdb;Persist Security Info=True;User ID=api;Password=Antymateria1410</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>